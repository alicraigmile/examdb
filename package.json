{
  "name": "examdb",
  "version": "2.6.0",
  "description": "A database of UK school examinations",
  "main": "src/server.js",
  "scripts": {
    "prelint": "npm run prettier",
    "deploy:prod": "npm run test && npm run version:patch && npm run push && npm run launch",
    "clean": "rm -rf build",
    "pretest": "npm run clean",
    "prettier": "prettier --tab-width=4 --single-quote --print-width=120 \"./src/**/*.js\" \"./test/**/*js\" \"./models/**/*js\" \"./routes/**/*js\" --write",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js src test",
    "version:patch": "npm version patch",
    "push": "npm run push:git && npm run push:heroku",
    "push:git": "git push -u --tags origin HEAD:master",
    "push:heroku": "git push heroku master",
    "launch": "open http://examdb.herokuapp.com",
    "prestart": "npm run db:setup",
    "db:setup": "node --require babel-core/register  --require babel-polyfill src/dbsetup.js",
    "start": "node --require babel-core/register  --require babel-polyfill src/server.js",
    "debug": "node --nolazy --inspect-brk=9229 --require babel-core/register  --require babel-polyfill src/server.js",
    "test": "npm run lint && npm run db:setup && npm run test:unit",
    "test:unit": "mocha --require babel-core/register --require babel-polyfill \"./test/**/*js\"",
    "changelog": "LASTTAG=`git describe --tags --abbrev=0`; echo \"======= Changes since last tag ($LASTTAG) =======\"; git log $LASTTAG..HEAD --oneline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alicraigmile/examdb.git"
  },
  "author": "Ali Craigmile <ali@craigmile.com> (http://ali.craigmile.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alicraigmile/examdb/issues"
  },
  "homepage": "https://github.com/alicraigmile/examdb#readme",
  "dependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "csv-express": "^1.2.2",
    "csv-parse": "^4.4.6",
    "express": "^4.14.0",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.1.10",
    "express-promise-router": "^3.0.3",
    "is-url": "^1.2.4",
    "jquery": "^3.5.0",
    "jquery-tablesort": "0.0.11",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "pg-promise": "^8.4.4",
    "prettier": "^1.13.7",
    "pug": "^3.0.1",
    "sequelize": ">=5.15.1",
    "serialize": "^0.1.3",
    "sqlite": "^3.0.0",
    "underscore": "^1.12.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  }
}
